{
	"name" : "Paywall Sledger", 
	"version" : "1.0", 
	"description" : "Chrome extension that sledges paywalls upon request.",
	"manifest_version" : 2,
	"background": {
		"scripts": ["background.js"], 
		"persistent": false
	},
	"icons": {
		"16" : "paywall_icon_16.png",
		"48" : "paywall_icon_48.png", 
		"128" : "paywall_icon_128.png"
	},
	"permissions": ["activeTab", "contextMenus"]
}
